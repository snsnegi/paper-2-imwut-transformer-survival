   journal = {Patient education and counseling},
   keywords = {Comprehension,Consumer Behavior,Danielle Schubbe,Delivery of Health Care / methods*,Health Behavior*,Health Education / methods*,Health Literacy*,Humans,MEDLINE,Marie-Anne Durand,Meta-Analysis,NCBI,NIH,NLM,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,Peter Scalia,PubMed Abstract,Systematic Review,doi:10.1016/j.pec.2020.04.010,pmid:32466864},
   month = {10},
   pages = {1935-1960},
   pmid = {32466864},
   publisher = {Patient Educ Couns},
   title = {Using pictures to convey health information: A systematic review and meta-analysis of the effects on patient and consumer health behaviors and outcomes},
   volume = {103},
   url = {https://pubmed.ncbi.nlm.nih.gov/32466864/},
   year = {2020},
}
