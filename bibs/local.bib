@article{Bleier2021,
   abstract = {Increasing privacy concerns regarding online behavioral advertising have sparked a renewed interest in online contextual advertising. However, while giving up on behavioral advertising may cede economic gains, adopting novel methods of contextual advertising may not cure consumer privacy. In fact, as such methods often extend far beyond the traditional matching of ads to keywords on a web page, they allow ads to become increasingly targeted to consumer information which may also violate consumer privacy and trigger privacy concerns. Firms and regulators should thus be careful not to inflict economic costs while failing to protect consumer privacy.},
   author = {Alexander Bleier},
   doi = {10.2139/SSRN.3980001},
   journal = {SSRN Electronic Journal},
   keywords = {Alexander Bleier,Privacy,SSRN,behavioral advertising,contextual advertising,contextual integrity},
   month = {12},
