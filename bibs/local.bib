   journal = {Proceedings of the ACM on Interactive, Mobile, Wearable and Ubiquitous Technologies},
   keywords = {Just-in-time intervention,interruptibility,prolonged sedentariness,receptivity},
   month = {6},
   pages = {1-26},
   publisher = {
		ACM
